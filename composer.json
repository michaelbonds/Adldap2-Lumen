{
    "name": "michaelb/adldap2-laravel",
    "description": "Adldap2 for Laravel 5.",
    "keywords": ["adldap", "adldap2", "ldap", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.0",
        "adldap2/adldap2": "6.0.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.2",
        "mockery/mockery": "0.9.*"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4": {
            "Adldap\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adldap\\Laravel\\Tests\\": "tests/"
        }
    }
}
